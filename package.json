{
  "name": "node-express-rest-api-starter",
  "version": "0.1.0",
  "description": "Basic starter for developing RESTful APIs using ExpressJS. Uses Passport local strategy for authentication and uses JWT for both API request authorization and connections to socket.io namespaces. Includes basic examples of mocha-chai tests.",
  "main": "app.js",
  "scripts": {
    "start": "node ./app.js",
    "dev": "nodemon ./app.js",
    "test": "mocha ./tests --recursive --exit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rledford/node-express-rest-api-starter.git"
  },
  "author": "rledford",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-rate-limit": "^3.1.0",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.2.10",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "socket.io": "^2.1.1",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3"
  }
}
